# Google Places API
GOOGLE_PLACES_API_KEY=your_key

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Search Configuration
SEARCH_LOCATION=Madrid, Spain
SEARCH_RADIUS=10000
BUSINESS_TYPES=restaurant,hair_care,beauty_salon

# Message Configuration
MESSAGE_TEMPLATE=Hola! Soy [TU_NOMBRE], especialista en páginas web para negocios locales. He visto que [NEGOCIO] tiene excelentes reseñas, especialmente por [PERSONALIZED_CONTENT]. Me gustaría mostrarle cómo una página web profesional podría ayudarle a atraer aún más clientes. ¿Tendría unos minutos para una breve conversación?

# Rate Limiting
REQUESTS_PER_MINUTE=10
DELAY_BETWEEN_MESSAGES=5000